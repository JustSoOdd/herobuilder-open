  "scripts": {
    "lint": "eslint src tools test",
    "test": "mocha --compilers js:babel/register --watch",
    "spritify": "gulp spritify",
    "update-assets": "npm run spritify && npm run data-merge",
    "clean": "babel-node --eval \"require('./tools/clean')().catch(err => console.log(err.stack))\"",
    "data-merge": "babel-node --eval \"require('./tools/merge-data')().catch(err => console.log(err.stack))\"",
    "build": "babel-node --eval \"require('./tools/build')().catch(err => console.log(err.stack))\"",
    "build:release": "babel-node --eval \"require('./tools/build')().catch(err => console.log(err.stack))\" release",
    "start": "babel-node --eval \"require('./tools/start')().catch(err => console.log(err.stack))\"",
    "deploy": "babel-node --eval \"require('./tools/deploy')().catch(err => console.log(err.stack))\"",
    "postinstall": "babel-node --eval \"require('./tools/build')().catch(err => console.log(err.stack))\" release"
  },
